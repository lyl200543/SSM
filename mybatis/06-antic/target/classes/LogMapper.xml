<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lyl.mybatis.mapper.logMapper" >
    <select id="selectByDate" resultType="com.lyl.mybatis.pojo.log">
        select *
        from t_log_${date}
    </select>
</mapper>
